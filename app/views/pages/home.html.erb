<div class="jumbotron center">
	<h1>Welcome to My App</h1>
	<% if user_signed_in? %>
		You're now logged in
	<% else %>
		<p>
			Click here to <%= link_to "Sign Up",new_user_registration_path, class: "btn btn-primary btn-lg" %>
			Or <%= link_to "Login",new_user_session_path, class: "btn btn-success btn-lg" %>
		</p>
	<% end %>
</div>